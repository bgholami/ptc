	rm -f Makefile
remove-genmakefiles:
	rm -f  Makefile
all-redirect: statusesf greqf

greqf: greqf.o dummyf.o   @MPILIBLOC@ ../util/mtestf.o
	$(F77_LINK) $(FFLAGS) $(LDFLAGS) -o greqf greqf.o dummyf.o  ${LIBS}
