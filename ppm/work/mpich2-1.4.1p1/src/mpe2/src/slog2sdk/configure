        chmod a+x $script
    else

if test "$enable_softerror" = "yes" ; then
    { $as_echo "$as_me:$LINENO: WARNING:  $script is not a valid file - Make sure you have configured with a valid $pkgname source directory " >&5
$as_echo "$as_me: WARNING:  $script is not a valid file - Make sure you have configured with a valid $pkgname source directory " >&2;}
    exit 0
else
    { { $as_echo "$as_me:$LINENO: error:  $script is not a valid file - Make sure you have configured with a valid $pkgname source directory " >&5
$as_echo "$as_me: error:  $script is not a valid file - Make sure you have configured with a valid $pkgname source directory " >&2;}
   { (exit 1); exit 1; }; }
fi

    fi
done

if test "$has_tracelib" = "yes" ; then
#   Rename all the scripts in bindir according to the TRACE_NAME
    if test "x$TRACE_NAME" != "x" -a "$TRACE_NAME" != "trace" ; then
        for oldpgm in $TRACE_PGMS ; do
            if test -x "bin/$oldpgm" ; then
                newpgm=$TRACE_NAME`echo $oldpgm | sed -e 's%trace%%'`
                $MV bin/$oldpgm bin/$newpgm
            fi
        done
    fi
fi

configured_pkgs=""
for pkg in $selected_pkgs ; do
    dirname=${pkg}_dirname
    eval dirvalue=\$"$dirname"
    if test -s $dirvalue/Makefile ; then
        configured_pkgs="$configured_pkgs \"$dirvalue\""
    fi
done

if test -z "$configured_pkgs" ; then
    { $as_echo "$as_me:$LINENO: WARNING: $pkgname: No sub-package has been configured" >&5
$as_echo "$as_me: WARNING: $pkgname: No sub-package has been configured" >&2;}
else
    { $as_echo "$as_me:$LINENO: result: $pkgname: $configured_pkgs have been configured" >&5
$as_echo "$pkgname: $configured_pkgs have been configured" >&6; }
fi
